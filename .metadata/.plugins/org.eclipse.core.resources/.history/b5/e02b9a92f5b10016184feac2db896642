package factory;

import movingobjects.Bullet;
import static main.GunRangePanel.*;

public class BulletConcreteFactory extends BulletFactory
{

	@Override
	Bullet createBullet(String mod, int offset) {
		// TODO Auto-generated method stub
		Bullet bullet;
		
		if (mod == "bullet")
		{
			bullet = new Bullet(WIN_W/2+(offset*50), WIN_H*0.8);
		}
		return null;
	}

}
