package pac;

import java.awt.*;

public class pacman 
{
	private int pacX, pacY;
	private int bodyW, bodyH;
	private int speedX;
	
	public pacman(int x, int y, int w, int h, int s)
	{
		pacX = x;
		pacY = y;
		bodyW = w;
		bodyH = h;
		speedX = s;
	}
	
	public void drawPacman(Graphics g)
	{
		Graphics2D g2 = (Graphics2D) g;
		g2.translate(pacX, pacY);
		
		g2.setColor(Color.yellow);
		
	}
}
