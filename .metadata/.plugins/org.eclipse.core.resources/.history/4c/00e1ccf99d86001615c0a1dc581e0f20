import java.awt.*;
import javax.swing.*;


public class Bubble {

	private Point pos;
	private int ySpeed;
	
	public Bubble(Point fish)
	{
		pos = fish;
		ySpeed = 5;
	}
	
	private void drawBubble(Graphics2D g2)
	{
		g2.setColor(Color.white);
		
		g2.fillOval(pos.x, pos.y, 20, 20);
	}
	
	private void move()
	{
		
	}

}
